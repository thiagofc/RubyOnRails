<h1>Colaborators per Manager</h1>

<table border="1">
  <thead>
    <tr>
      <th>Colaborators</th>
    </tr>
  </thead>
  <tbody>
    <% @manager.colaborators.each do |colaborator| %>
        <tr>
          <td>
            <%= link_to colaborator.name, controller: "hours_registrations", action: "list_hours_colaborator", id: colaborator.id %>
          </td>
        </tr>
    <% end %>
  </tbody>
</table>

<br>